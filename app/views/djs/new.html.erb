<h1>New Dj</h1>

<%= form_tag(djs_url, method: 'post') do %>
  <div>
    <%= label_tag :name %><br />
    <%= text_field_tag :name, nil, placeholder: "Name" %>
  </div>

  <div>
    <%= label_tag :crew %><br />
    <%= text_field_tag :crew, nil, placeholder: "Crew" %>
  </div>

  <div>
    <%= submit_tag "Create Dj" %>
  </div>
<% end %>
